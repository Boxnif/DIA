<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.KM.boxnif.dia.Modul_1">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/m1ueberschrift"
        android:id="@+id/ueberschrift"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="bold"
        android:textSize="26dp"
        android:background="@android:color/background_light"/>
    <Button
        android:id="@+id/ibtn"
        android:text="@string/qest"
        android:background="@android:drawable/checkbox_off_background"
        android:layout_width="24dp"
        android:layout_height="26dp"
        android:textSize="18dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

    <View
        android:id="@+id/view"
        android:layout_width="fill_parent"
        android:layout_height="3dip"
        android:background="#000000"
        android:layout_below="@+id/ueberschrift"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView1"
        android:layout_below="@+id/view">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m101"
                android:id="@+id/textView1"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@+id/ibtn1"/>

            <Button
                android:id="@id/ibtn1"
                android:text="@string/qest"
                android:layout_alignParentEnd="false"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@id/textView1"
                android:textSize="18dp"
                android:layout_width="24dp"
                android:layout_height="26dp"/>

            <Spinner
                android:id="@+id/spinner1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView1"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/selbststaendig_array"/>
            <View
                android:id="@+id/view1"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_below="@id/spinner1"
                android:layout_marginBottom="20dp"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m102"
                android:id="@+id/textView2"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@+id/ibtn2"
                android:layout_below="@id/view1"/>

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/selbststaendig_array"/>
            <View
                android:id="@+id/view2"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_below="@id/spinner2"
                android:layout_marginBottom="20dp"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m103"
                android:id="@+id/textView3"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@id/ibtn1"
                android:layout_below="@id/view2"/>

            <Button
                android:id="@+id/ibtn3"
                android:text="@string/qest"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false"
                android:layout_below="@id/view2"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_width="24dp"
                android:layout_height="26dp"
                android:textSize="18dp"/>

            <Spinner
                android:id="@+id/spinner3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/selbststaendig_array"/>
            <View
                android:id="@+id/view3"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_below="@+id/spinner3"
                android:layout_marginBottom="20dp"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m104"
                android:id="@+id/textView4"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@+id/ibtn4"
                android:layout_below="@id/view3"/>

            <Button
                android:id="@id/ibtn4"
                android:text="@string/qest"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false"
                android:layout_below="@id/view3"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_width="24dp"
                android:layout_height="26dp"
                android:textSize="18dp"/>

            <Spinner
                android:id="@+id/spinner4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/selbststaendig_array"
                />
            <View
                android:id="@+id/view4"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_below="@id/spinner4"
                android:layout_marginBottom="20dp"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m105"
                android:id="@+id/textView5"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@id/ibtn1"
                android:layout_below="@id/view4"/>

            <Button
                android:id="@+id/ibtn5"
                android:text="@string/qest"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false"
                android:layout_below="@id/view4"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_width="24dp"
                android:layout_height="26dp"
                android:textSize="18dp"/>

            <Spinner
                android:id="@+id/spinner5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView5"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/selbststaendig_array"/>
            <View
                android:id="@+id/view5"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_below="@id/spinner5"
                android:layout_marginBottom="20dp"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/m106"
                android:id="@+id/textView6"
                android:layout_alignParentStart="true"
                android:layout_toStartOf="@id/ibtn1"
                android:layout_below="@id/view5"/>

            <Button
                android:id="@+id/ibtn6"
                android:text="@string/qest"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/view5"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_width="24dp"
                android:layout_height="26dp"
                android:textSize="18dp"/>

            <Spinner
                android:id="@+id/spinner6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textView6"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="false"
                android:entries="@array/janein_array"
                android:focusable="false"/>
            <View
                android:id="@+id/view6"
                android:layout_width="fill_parent"
                android:layout_height="3dip"
                android:background="#000000"
                android:layout_below="@id/spinner6"
                android:layout_marginBottom="20dp"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/weiter"
                android:id="@+id/modul_1_button"
                android:layout_below="@id/view6"
                android:layout_centerHorizontal="true"/>

            <Button
                android:id="@id/ibtn2"
                android:layout_width="24dp"
                android:layout_height="26dp"
                android:text="@string/qest"
                android:background="@android:drawable/checkbox_off_background"
                android:layout_below="@+id/view1"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:textSize="18dp"/>

        </RelativeLayout>
    </ScrollView>



</RelativeLayout>
